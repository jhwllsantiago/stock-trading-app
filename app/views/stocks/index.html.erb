<p style="color: green"><%= notice %></p>

<h1>Stocks</h1>

<div id="stocks">
  <% @stocks.each do |stock| %>
    <%= render stock %>
    <p>
      <%= link_to "Show this stock", stock_details_path(stock) %>
    </p>
  <% end %>
</div>

<%= button_to "Sign out", destroy_user_session_path, :method => :delete %>
